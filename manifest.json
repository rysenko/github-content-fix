{
  "manifest_version": 2,
  "name": "Github Content Fix",
  "version": "1.0.3",
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://github.com/*",
    "https://*.github.com/*"
  ]
}